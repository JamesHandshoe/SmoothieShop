		<property name="type" value="VEGETABLE"/>
		<property name="needsWash" value="true"/>
	</bean>
	   
	<bean id="spinach" class="org.elevenfifty.smoothie.beans.Produce" scope="prototype">
		<property name="name" value="Spinach"/>
		<property name="cost" value="0.20"/>
		<property name="type" value="VEGETABLE"/>
		<property name="needsWash" value="true"/>
	</bean>
	   
	<bean id="ice" class="org.elevenfifty.smoothie.beans.Ingredient" scope="prototype">
		<property name="name" value="Ice"/>
		<property name="type" value="MISC"/>
	</bean>
	   
	<bean id="water" class="org.elevenfifty.smoothie.beans.Ingredient" scope="prototype">
		<property name="name" value="Water"/>
		<property name="type" value="BASE"/>
	</bean>
	   
	<bean id="milk" class="org.elevenfifty.smoothie.beans.Ingredient" scope="prototype">
		<property name="name" value="Milk"/>
		<property name="cost" value="0.1"/>
		<property name="type" value="BASE"/>
	</bean>
	   
	<bean id="yogurt" class="org.elevenfifty.smoothie.beans.Ingredient" scope="prototype">
		<property name="name" value="Yogurt"/>
		<property name="cost" value="0.1"/>
		<property name="type" value="BASE"/>
	</bean>

</beans>
