	<bean id="mango" class="org.elevenfifty.smoothie.beans.Produce" scope="prototype">
		<property name="name" value="Mango"/>
		<property name="cost" value="1.0"/>
		<property name="type" value="FRUIT"/>
		<property name="peelable" value="true"/>
		<property name="pitable" value="true"/>
	</bean>
	   
	<bean id="guava" class="org.elevenfifty.smoothie.beans.Produce" scope="prototype">
		<property name="name" value="Guava"/>
		<property name="cost" value="1.0"/>
		<property name="type" value="FRUIT"/>
		<property name="peelable" value="true"/>
